#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/wuting/Public/study/study-react/node_modules/.pnpm/jest@27.5.1/node_modules/jest/bin/node_modules:/Users/wuting/Public/study/study-react/node_modules/.pnpm/jest@27.5.1/node_modules/jest/node_modules:/Users/wuting/Public/study/study-react/node_modules/.pnpm/jest@27.5.1/node_modules:/Users/wuting/Public/study/study-react/node_modules/.pnpm/node_modules:/Users/wuting/Public/study/study-react/node_modules:/Users/wuting/Public/study/node_modules:/Users/wuting/Public/node_modules:/Users/wuting/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/wuting/Public/study/study-react/node_modules/.pnpm/jest@27.5.1/node_modules/jest/bin/node_modules:/Users/wuting/Public/study/study-react/node_modules/.pnpm/jest@27.5.1/node_modules/jest/node_modules:/Users/wuting/Public/study/study-react/node_modules/.pnpm/jest@27.5.1/node_modules:/Users/wuting/Public/study/study-react/node_modules/.pnpm/node_modules:/Users/wuting/Public/study/study-react/node_modules:/Users/wuting/Public/study/node_modules:/Users/wuting/Public/node_modules:/Users/wuting/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/jest.js" "$@"
else
  exec node  "$basedir/../../bin/jest.js" "$@"
fi
